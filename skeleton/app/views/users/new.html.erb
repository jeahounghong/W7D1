<h2>Create your credentials:</h2>


<form action="<%=users_url%>" method="post">
    <label>Username:
        <input type="text" value="<%=@user.user_name%>" name="user[user_name]">
    </label>

    <label>Password:
        <input type="password" value="<%=@user.password%>" name="user[password]">
    </label>

    <input type="submit" value="Sign Up">
</form>